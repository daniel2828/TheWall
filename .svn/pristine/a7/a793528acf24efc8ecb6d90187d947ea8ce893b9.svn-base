    getDatosTwitter: function (idCM, tipoRS, lastID) {

        //vaciamos la array temporalmente
        //app.main.tempArrayPosts = {};         var url = "";

        // Llamamos al proxi.php con la url que queramos para hacer la peticion a twitter
        // Gestiono que se pueda pasar tanto una id como un nombre de usuario!

        //TODO-- Hay que optimizar el codigo 
        // Poner url dependiente de lso parametros y solo una llamada 

        var url = "count=100";
        if (isNaN(idCM)) {
            if (lastID == "" || lastID == null || lastID == 0) {
                url += '&screen_name=' + idCM;
            }
            else {
                url += '&screen_name=' + idCM + '&max_id=' + lastID;
            }
        } else {
            if (lastID == "" || lastID == null || lastID == 0) {
                url += '&user_id=' + idCM;
            }
